		<http:request-connection protocol="HTTPS" host="${sfdc.auth.host}" />
	</http:request-config>
	<http:request-config name="SFDC_ENDPOINT" doc:name="HTTP Request configuration" doc:id="ae4fa5ee-1b70-4f17-b426-a548d846774e" >
